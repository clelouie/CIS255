public class Team {
	private String name;
	private int wins;
	private int losses;
   
	// Mutator method
	// Takes string parameter userName, sets  name to userName 
	public void setName(String userName) {
		name = userName;
	}
	
	
	
	
	
	// Mutator method
	// Takes integer parameter userWinds, sets wins to userWins
	public void setWins(int userWins) {
		wins = userWins;
	}
	
	
	
	
	
   
	// Mutator method
	// Takes integer parameter userLosses, sets losses to userLosses.
	public void setLosses(int userLosses) {
		losses = userLosses;
	}
	
	
	
	
	
   
   // TODO: Define accessor methods - 
   //       getName(), getWins(), getLosses()
	public String getName() {
		return name;
	}
   
	
	
	
	
	
   public int getWins() {
	   return wins;
   }
   
   
   
   
   
   
   public int getLosses() {
	   return losses;
   }
   
   
   
   
   
   
   // TODO: Define getWinPercentage()
   public double getWinPercentage() {
	   return wins / (double)(wins + losses);
   }
   
   
   // TODO: Define printStanding()
   
}